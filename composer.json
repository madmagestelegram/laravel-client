{
  "name": "madmagestelegram/laravel",
  "type": "library",
  "description": "Telegram bot integration with laravel",
  "keywords": [
    "laravel",
    "telegram",
    "bot",
    "rpc"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.4",
    "madmagestelegram/client": "*",
    "illuminate/support": "^6 || ^7 || ^8",
    "illuminate/http": "^6 || ^7 || ^8"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "MadmagesTelegram\\Laravel\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MadmagesTelegram\\Laravel\\ServiceProvider"
      ]
    }
  }
}
